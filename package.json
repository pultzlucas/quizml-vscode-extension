{
    "name": "quizml",
    "displayName": "QuizML",
    "description": "HighLighting extension to the Quiz Markup Language",
    "version": "0.2.1",
    "icon": "icon.png",
    "homepage": "https://github.com/Lucas-Lopes-Pultz/quizml-vscode-extension/#README.md",
    "publisher": "pultzlucas",
    "repository": {
        "type": "git",
        "url": "https://github.com/Lucas-Lopes-Pultz/quizml-vscode-extension.git"
    },
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "quizml",
        "quiz",
        "Quiz Markup Language"
    ],
    "contributes": {
        "languages": [
            {
                "id": "quizml",
                "aliases": [
                    "Quiz Markup Language",
                    "quizml"
                ],
                "extensions": [
                    ".quiz"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "quizml",
                "scopeName": "source.quiz",
                "path": "./syntaxes/quizml.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "quizml",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}