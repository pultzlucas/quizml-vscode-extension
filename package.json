{
    "name": "quizml",
    "displayName": "QuizML",
    "description": "HighLighting extension to the Quiz Markup Language",
    "version": "0.0.1",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Lucas-Lopes-Pultz/quizml-vscode-extension"
    },
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "quizml",
            "aliases": ["Quiz Markup Language", "quizml"],
            "extensions": [".quiz"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "quizml",
            "scopeName": "source.quiz",
            "path": "./syntaxes/quizml.tmLanguage.json"
        }]
    }
}