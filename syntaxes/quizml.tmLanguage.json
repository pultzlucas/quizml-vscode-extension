{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Quiz Markup Language",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.quizml",
				"match": "->|<-"
			}]
		},
		"strings": {
			"name": "string.quoted.double.quizml",
			"begin": "",
			"end": "\n",
			"patterns": [
				{
					"name": "constant.character.escape.quizml",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.quiz"
}